{
    "inputs": [
        {
            "name": "Attack",
            "min": 0,
            "max": 250,
            "step": 1,
            "units": "milliseconds",
            "value": 20
        },
        {
            "name": "Decay",
            "min": 0,
            "max": 250,
            "step": 1,
            "units": "milliseconds",
            "value": 100
        }
    ],
    "groups": [
        "0-250 (Hz)",
        "250-500 (Hz)",
        "500-750 (Hz)",
        "750-1k (Hz)",
        "1k-2k (Hz)",
        "2k-4k (Hz)",
        "4k-12k (Hz)",
        "12k-24k (Hz)"
    ],
    "processing": {
        "initFunctions":[
            {"output_name":"table_attack_decay", "function":"[]"}
        ],
        "groupFunctions": [
            {"output_name":"test2", "function":"print_table([tau_a_${index}])"},
            {"output_name":"test3", "function":"print_table([tau_r_${index}])"},
            {"output_name":"table_${index}_attack_decay", "function":"print_table([exp(-1/(fs*tau_a_${index}/1000)), exp(-1/(fs*tau_r_${index}/1000))])"}
        ],
        "endFunctions": [
        ],
        "inputs": [
            {
                "name":"tau_a_${index}",
                "type":"pointer",
                "value":0
            },
            {
                "name":"tau_r_${index}",
                "type":"pointer",
                "value":1
            },
            {
                "name":"fs",
                "type":"constant",
                "value":48000
            },
            {
                "name":"print_table",
                "type":"function",
                "value": {
                    "parameters":["table"],
                    "type":"javascript",
                    "output": "let output = ''; table.map(value => value.toFixed(5).replace(/\\.0+$/,'')).forEach((val) => {output += val + ' ';}); console.log(output); return output;"
                }
            }
        ],
        "output": "%table_0_attack_decay%table_1_attack_decay%table_2_attack_decay%table_3_attack_decay%table_4_attack_decay%table_5_attack_decay%table_6_attack_decay%table_7_attack_decay"
	}
}